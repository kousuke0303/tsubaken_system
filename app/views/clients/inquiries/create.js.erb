$(".inquiry-create-errors").html('<%= escape_javascript(render "shared/error_messages", resource: @inquiry) %>');
<% @inquiry.errors.messages.keys.each do |key_attribute| %>
  var addDiv = $('<div class="field-with-errors">');
  var addLabel = $('[name="inquiry[<%= key_attribute.intern.to_s %>]"]').prev();
  $('[name="inquiry[<%= key_attribute.intern.to_s %>]"]').wrap(addDiv);
  addLabel.wrap(addDiv);
<% end %>
