<div class="container">
  <h5>取引関係者：<%= @supplier.company %>の編集</h5>
  <div class="edit_frame">
    <%= form_with(model: [:manager, @supplier], url: manager_supplier_path(dependent_manager, @supplier), local: true) do |f| %>
      <%= render partial: 'manager/suppliers/form', locals: { f: f } %>
    <% end %>
  </div>
</div>

<div class="modal address_modal">
  <div class="modal__bg js-modal-close"></div>
  <div class="address_select">
  </div>
</div>

<script>
$('.prefectures_all').on('click', function(){
  $.ajax({
    type: "GET",
    url: "<%= prefecture_index_path(dependent_manager) %>",
    cache: false,
    data: {remote: true}
  });
})
</script>