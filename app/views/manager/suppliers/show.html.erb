<div class="container">
  <h5>取引関係者：<%= @supplier.company %>の詳細</h5>
  <div class="table-responsive">
    <table class="table">
      <tr>
        <th>会社名</th>
        <td><%= @supplier.company %></td>
      </tr>
      <tr>
        <th>所在地</th>
        <td><%= @supplier.location %></td>
      </tr>
      <tr>
        <th>代表者</th>
        <td><%= @supplier.representative_name %></td>
      </tr>
      <tr>
        <th>電話番号</th>
        <td><%= @supplier.phone %></td>
      </tr>
      <tr>
        <th>FAX</th>
        <td><%= @supplier.fax %></td>
      </tr>
      <tr>
        <th>Mail</th>
        <td><%= @supplier.mail %></td>
      </tr>
    </table>
  </div>
  <%= link_to "編集", "", class: "btn btn-dark float-r" %>
</div>