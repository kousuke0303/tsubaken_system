var dom = "<%= dom %>";
$(dom).html('<%= escape_javascript(render "shared/error_messages", resource: model) %>');

<% model.errors.messages.keys.each do |key_attribute| %>
  <% unless key_attribute.to_s == "accept" %>
    var taget_dom = $('[name="<%= object %>[<%= key_attribute.to_s %>]"]')
    var addDiv = $('<div class="field-with-errors mt-05e">');
    var addLabel = $('[name="<%= object %>[<%= key_attribute.to_s %>]"]').prev();
    $('[name="<%= object %>[<%= key_attribute.to_s %>]"]').wrap(addDiv);
    addLabel.wrap(addDiv);
  <% else %>
    $('.accept_text').addClass('text-danger')
  <% end %>
<% end %>