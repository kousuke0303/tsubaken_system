<script>
  $(document).ready(function () {
    $("#tableCollapse").addClass('active');
    $(".responsive_table_sidebar").addClass('active');
    $("#tableCollapse").on("click", function () {
      $(".responsive_table_sidebar").toggleClass("active");
      $(this).toggleClass("active");
    });
  });
</script>

