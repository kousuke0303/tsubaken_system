<div class="row">
  <% @images.each do |image| %>
    <div class="col-sm-2 col-12">
      <%= link_to new_employees_estimate_matter_certificate_path(@estimate_matter, image_id: image.id), remote: true do %>
        <div class="row image-item employees-image-item">
          <%= image_tag url_for(image.images[0].variant(resize: "150x150"))%>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
