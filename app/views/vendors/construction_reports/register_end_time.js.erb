<% if @responce == "success" %>
  <% if current_vendor_manager %>
    $('#schedule-today').html('<%= escape_javascript(render partial: "vendors/construction_schedules/partial/construction_schedule",
                                                            locals: {construction_schedules: @construction_schedules_for_today,
                                                                     matter_path: :vendor_managers_matter_path }) %>');
    toastr.info('本日の報告をお願いします');
  <% end %>

  <% if current_external_staff %>
    $('#schedule-today').html('<%= escape_javascript(render partial: "vendors/construction_schedules/partial/construction_schedule",
                                                            locals: {construction_schedules: @construction_schedules_for_today,
                                                                     matter_path: :external_staffs_matter_path }) %>');
    toastr.info('本日の報告をお願いします');
  <% end %>

<% end %>
