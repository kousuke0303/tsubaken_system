<% if @responce == "success" %>
  $(".reports-index").html('<%= escape_javascript(render "employees/matters/partial/reports") %>');
  <%= render "shared/javascript/flash", dom: "#report-flash", key: "success", value: "#{ @report.title }を削除しました。" %>
<% else %>
  <%= render "shared/javascript/flash", dom: "#report-flash", key: "alert", value: "#{ @report.title }を削除できませんでした。" %>
<% end %>
