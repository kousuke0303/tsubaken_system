<% if @responce == "success" %>
  $(".attract-methods-index").html('<%= escape_javascript(render "employees/settings/companies/attract_methods/partial/index_table") %>');
  <%= render "shared/javascript/flash", dom: ".flash-wrapper", key: "success", value: "#{ @attract_method.name }を作成しました" %>
<% else %>
  toastr.error("<%= params[:attract_method][:name] %>は既に存在します");
<% end %>