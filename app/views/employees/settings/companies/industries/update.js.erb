<% if @responce == "success" %>
  $("#modal-container").modal("hide");
  $(".industries-index").html('<%= escape_javascript(render "employees/settings/companies/industries/partial/index_table") %>');
  toastr.success("<%= @industry.name %>に変更しました");
  $("#modal-container").empty();
<% else %>
  <%= render partial: 'shared/javascript/modal_error', locals: {dom: ".industry-update-errors", model: @industry, object: "industry"} %>
<% end %>
  