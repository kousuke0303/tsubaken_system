<% if @responce == "success" %>
  $('#pass-form').html('<%= escape_javascript(render 'employees/supplier_managers/partial/pass_form') %>');
  toastr.success('#{@supplier_manager.name}のパスワードを更新しました');
<% else %>
  <%= render partial: 'shared/javascript/modal_error', locals: {dom: ".pass-word-errors", model: @supplier_manager, object: "supplier_manager"} %>
<% end %>