<div class="card">
  <div class="card-body">
    <%= form_with(model: @supplier_manager, url: employees_supplier_managers_path(id: @supplier.id), method: :post, remote: true) do |f| %>
      <div id="supplier-manager-create-errors"></div>
      <%= render 'supplier_managers/registrations/partial/form', f: f %>
      <div class="btn-area">
        <%= f.submit "登録", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>