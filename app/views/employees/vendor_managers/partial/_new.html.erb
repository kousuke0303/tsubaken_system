<div class="card">
  <div class="card-body">
    <%= form_with(model: @vendor_manager, url: employees_vendor_managers_path(id: @vendor.id), method: :post, remote: true) do |f| %>
      <div id="vendor-manager-create-errors"></div>
      <%= render 'vendor_managers/registrations/partial/form', f: f %>
      <div class="btn-area">
        <%= f.submit "登録", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>