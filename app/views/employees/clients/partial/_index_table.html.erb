<section class="mt-1e pagenation_area">
  <%= will_paginate objects, :params=>{type: type} %>
<section>

<% objects.each do |client| %>
  <tr class="tr-link" data-link="<%= employees_client_path(client) %>">
    <td><%= client.name %></td>
    <td><%= client.login_id %></td>
    <td><%= phone_formatted(client.phone_1) %></td>
    <td><%= client.email %></td>
  </tr>
<% end %>

<%= javascript_include_tag "tr_link.js" %>
